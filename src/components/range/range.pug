.range_container
    .label Range slider
    span.label_right 5 000₽ - 10 000₽
    .range-slider
        input#left-range.input_range(type='range' min='0' max='100' value='25')
        input#right-range.input_range(type='range' min='0' max='100' value='75')
        .slider
            .track
            .range
            .thumb.thumb_left
            .thumb.thumb_right
