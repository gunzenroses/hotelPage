{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "Main": "./src/",
          "Fonts": "./src/assets/fonts/",
          "Images": "./src/assets/images/",
          "Scripts": "./src/scripts/",
          "Styles": "./src/assets/styles/",
          "Components": "./src/components/",
          "Pages": ".src/pages/"
        }
      }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ]
  ]
}
